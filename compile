nasm -f bin ./src/bootloader/boot.asm -o ./build/binary/boot.bin
dd if=/dev/zero of=./build/image/boot.img bs=1024 count=1440
dd if=./build/binary/boot.bin of=./build/image/boot.img seek=0 count=1 conv=notrunc
cp ./build/image/boot.img ./build/image/boot.flp
mkisofs -no-emul-boot -boot-load-size 4 -o ./build/image/16RM-OS.iso -b boot.flp ./build/image/
